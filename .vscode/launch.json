  "version": "0.2.0",
  "configurations": [
    {
      "name": "ng serve",
      "type": "pwa-chrome",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "url": "http://localhost:4200/"
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Karma",
      "address": "localhost",
      "port": 9223,
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
    },
  ]
}
